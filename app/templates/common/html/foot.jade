
if state.modulescss
  for name in state.modulescss
    hrf = "css/modules/" + name + ".css"
    link(rel="stylesheet", href= hrf)

script(src='/bower_components/requirejs/require.js')

- for ( i in state.javascriptModules )
  - state.javascriptModules[i] = '/js/modules/' + state.javascriptModules[i] + '/index.js'

script.
  require(["/js/common.js"], function () {
    require(["secondary/index"], function(){
      require(!{JSON.stringify(state.javascriptModules)});
    });
  });

- delete state.modulescss
- delete state.javascriptModules

</html>
